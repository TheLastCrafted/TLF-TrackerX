{
  "expo": {
    "name": "TLF TrackerX",
    "slug": "tlf-trackerx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-v2.png",
    "scheme": "tlftrackerx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon-v2.png",
      "bundleIdentifier": "com.daniel.tlftrackerx",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.daniel.tlftrackerx",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon-v2.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "notification": {
      "icon": "./assets/images/icon-v2.png",
      "color": "#6D4BFF"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/web-tab-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-v2.png",
          "color": "#6D4BFF",
          "defaultChannel": "price-alerts",
          "sounds": []
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-v2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f0f33658-6ce2-4053-812b-993ed3bc4e61"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f0f33658-6ce2-4053-812b-993ed3bc4e61"
    }
  }
}
